[
  {
    "id":"group-id",
    "title":"Group title",
    "sections":[
      {
        "id":"mainSection",
        "fields":[
          {
            "id":"email",
            "title":"Email",
            "type":"text",
            "size":{
              "width":60,
              "height":1
            },
            "validations":{
              "format":"[\\w._%+-]+@[\\w.-]+\\.\\w{2,}",
              "required":true
            }
          },
          {
            "id":"password",
            "title":"Password",
            "type":"password",
            "input_type":"password",
            "size":{
              "width":60,
              "height":1
            },
            "validations":{
              "min_length":6,
              "required":true
            }
          },
          {
            "id":"login",
            "title":"Login",
            "type":"button",
            "disabled":true,
            "size":{
              "width":60,
              "height":1
            }
          }
        ]
      }
    ]
  }
]
